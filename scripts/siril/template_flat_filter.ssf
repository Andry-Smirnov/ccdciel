############################################
# Script for Siril
# Template for CCDciel preprocessing 
# Create master flat with filter
############################################

setext fits

cd $BASEDIR

#preprocess flats
cd $FLATDIR
preprocess $FLATPREFIX_$FILTER_ -bias=$MASTERBIAS
#build master-flat
stack pp_$FLATPREFIX_$FILTER_ rej 3 3 -norm=mul

load pp_$FLATPREFIX_$FILTER_stacked.fits
save Master_Flat_$FILTER.fits

close
